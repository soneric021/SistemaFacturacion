@model IEnumerable<CapaEntidades.Stock>

@{
    ViewBag.Title = "Lista de Stocks";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar productos", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.producto.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.proveedor.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.producto.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.proveedor.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
     
    </tr>
}

</table>
